        var flag: String {
            switch self {
            case .japanese: return "ğŸ‡¯ğŸ‡µ"
            case .english: return "ğŸ‡ºğŸ‡¸"
            case .chineseSimplified: return "ğŸ‡¨ğŸ‡³"
            case .chineseTraditional: return "ğŸ‡¹ğŸ‡¼"
            case .vietnamese: return "ğŸ‡»ğŸ‡³"
